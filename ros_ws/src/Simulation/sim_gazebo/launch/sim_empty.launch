<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find sim_gazebo)/world/empty.world"/>
  </include>

	<!-- Spawn a robot into Gazebo -->
	<node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find robot_description)/urdf/robot.urdf -urdf -model robot"	
    output="screen" />

</launch>
